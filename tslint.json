{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-react-hooks"
  ],
  "rules": {
    "no-empty": false,
    "no-submodule-imports": false,
    "indent": [true, "spaces", 2],
    "max-line-length": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "jsx-double"],
    "no-empty-interface": false
  }
}